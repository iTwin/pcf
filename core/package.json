{
  "name": "@itwin/pcf",
  "version": "0.1.2",
  "description": "Define your digital twin as code with iTwin PCF (Parametric Connector Framework).",
  "main": "lib/pcf.js",
  "typings": "lib/pcf",
  "license": "MIT",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel"
  ],
  "engines": {
    "node": ">=12.17.0 <15.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "homepage": "https://github.com/itwin/pcf",
  "repository": {
    "type": "git",
    "url": "https://github.com/itwin/pcf"
  },
  "scripts": {
    "build": "tsc && npm run copyFiles",
    "clean": "rimraf lib",
    "lint": "tslint --project .",
    "test": "npm run test:unit",
    "test:unit": "mocha --opts ./src/test/unit/mocha.opts \"./src/test/unit/**/*.test.ts*\"",
    "test:integration": "mocha --opts ./src/test/unit/mocha.opts \"./src/test/integration/**/*.test.ts*\"",
    "copyFiles": "npx babel src --out-dir lib --copy-files",
    "deploy": "git push && npm run clean && npm run build && npm publish",
    "docs": "betools docs"
  },
